<div class="row">
	<div class="col s12">
		<h1>Categoria:<%= category %></h1>

		<ul class="collection">
			<% for(var i=0; i < articles.length; i++) { var article = article; %>
				<li class="collection-item">
					<a href="#!" onclick="apretaste.send({'command':'CUBANET HISTORIA','data':{'historia':'<%= article.link %>', 'title':'<%= article.title %>'}})">
						<h6><%= article.title %></h6>
					</a>

					<% if(articles[i].description.length > 0){ %>
						<p><%= article.description %></p>
					<% } %>
				</li>
			<% } %>
		</ul>

		<div class="fixed-action-btn">
			<a class="btn-floating btn-large waves-effect waves-light" href="#!" onclick="apretaste.send({'command':'CUBANET'})"><i class="material-icons">arrow_back</i></a>
		</div>
	</div>
</div>