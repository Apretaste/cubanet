<div class = "row">
    <div class = "col s12 m12 xl12">
        <h1>Buscar: <% search %></h1>

        <% articles.forEach(function(article) { %>
            <small>
                <%= formatDate(article.pubDate) %>
            </small>
            <br/>
            <b><a href = "#!" onclick = "apretaste.send({command: 'CUBANET HISTORIA', data: {query: article.link}})"><%= article.title %></a></b>
            <%= article.description.substr(0, 200) %><br/>
        <% }); %>

        <button class = "btn waves-effect waves-light" onclick = "apretaste.send({command:'CUBANET'})">
            Titulares
            <i class = "material-icons right">feed</i>
        </button>
    </div>
</div>